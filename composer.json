{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "arrilot/laravel-widgets": "^3.13",
        "fideloper/proxy": "^4.2",
        "fomvasss/laravel-dadata": "^2.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/glide-laravel": "^1.0",
        "spatie/laravel-ray": "^1.31"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "barryvdh/laravel-ide-helper": "^2.6",
        "composer/composer": "^1.10@dev",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "overtrue/phplint": "^4.3",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "vendor/bin/phplint --no-cache",
        "test": "./vendor/bin/phpunit --colors=always --stop-on-failure",
        "pint": "./vendor/bin/pint",
        "pinttest": "./vendor/bin/pint --test",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.d-element.ru/fomvasss/laravel-dadata.git"
        }
    ]
}
